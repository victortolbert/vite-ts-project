  "scripts": {
    "clean": "rimraf dist",
    "dev": "vite --port 8888",
    "build": "vite build",
    "seed": "node seed",
    "start": "node server",
    "start2": "node server2",
    "deploy": "now",
    "serve": "vite preview --port 5050",
    "preview": "vite preview --port 8898 --host",
    "preview-https": "PORT=4433 serve dist",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext=.ts,.vue",
    "lint:js": "eslint \"**/*.{vue,ts,js}\"",
    "lint:css": "stylelint src/assets/css/**/*.css",
    "lint:css:fix": "stylelint src/css/**/*.css --fix",
    "lint:sass": "stylelint \"**/*.scss\" --fix",
    "analysis": "windicss-analysis",
    "preserve": "vite build",
    "coverage": "vitest run --coverage",
    "test": "vitest",
    "test:related": "vitest related /Scripts/Common/Main.ts",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:unit": "vitest",
    "test:unit:ci": "cypress run-ct --quiet --reporter spec",
    "test:integration": "cypress open",
    "test:e2e": "start-server-and-test serve 5050 'cypress open'",
    "test:e2e:ci": "start-server-and-test serve 5050 'cypress run'",
    "cy": "cypress open-ct",
    "cy:ci": "cypress run-ct --quiet --reporter spec",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "config-viewer": "npx tailwind-config-viewer",
    "build:bulma": "sass src/assets/sass/bulma.scss public/assets/css/bulma.css",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "fractal:start": "fractal start --sync",
    "sassdoc": "sassdoc src/assets/sass/config --theme herman",
    "sassdoc1": "sassdoc src/assets/scss/**/*.scss",
    "site:clean": "rimraf './_site/!(assets)' './_site/assets/!(images)'",
    "assets:clean": "rimraf './_built/'",
    "up": "taze major -I"
  },
